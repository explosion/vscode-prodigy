{
	"name": "vscode-prodigy",
	"displayName": "Prodigy",
	"description": "A Visual Studio Code extension for annotating machine learning training sets using Prodigy",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-prodigy.start"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-prodigy.start",
				"title": "Open Prodigy"
			}
		],
		"configuration": {
			"title": "Prodigy",
			"properties": {
				"vscode-prodigy.url": {
					"type": "string",
					"default": null,
					"description": "Prodigy URL, useful if you're using a reverse proxy (e.g. https://prodigy.example.com)"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}